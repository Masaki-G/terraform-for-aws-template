[
  {
    "name" : "backend",
    "image": "700562912253.dkr.ecr.ap-northeast-1.amazonaws.com/dev-reborn-api:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8000
      }
    ],
    "secrets": [
      {
        "valueFrom": "/dev/db/host",
        "name": "MYSQL_HOST"
      },
      {
        "valueFrom": "/dev/db/port",
        "name": "MYSQL_PORT"
      },
      {
        "valueFrom": "/dev/db/username",
        "name": "MYSQL_USER"
      },
      {
        "valueFrom": "/dev/db/password",
        "name": "MYSQL_PASSWORD"
      },
      {
        "valueFrom": "/dev/db/database",
        "name": "MYSQL_DATABASE"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "django",
        "awslogs-group": "/ecs/example"
      }
    }
  },
    {
    "name" : "web-backend-nginx",
    "image": "700562912253.dkr.ecr.ap-northeast-1.amazonaws.com/web-backend-nginx:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
      "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "nginx",
        "awslogs-group": "/ecs/example"
      }
    }
    }
]